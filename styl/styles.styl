*
	margin 0
	padding 0
	outline none
body
	font-family: "OpenSansRegular", sans-serif;
	background-color: #F6F7F9
a
	color: #fefefe
	&:focus
		outline: 0
	&:hover
		text-decoration none
button
	border none
.video_wrapper
	margin 0 auto
	position relative
	&:before
		content ''
		height 0
		padding-top 56.25%
		display block
	&:after
		content: ''
		display block
		position: absolute
		top: 0
		left 0
		right 0
		width: 100%
		height 70px
	img,
	iframe
		position absolute
		width 100%
		height 100%
		display block
		background-color #fff
		border-radius 3px
		border: 0
		top 0
		left 0
		right 0
		bottom 0
		cursor pointer
.button
	display block
	background-image: linear-gradient( 0deg, rgb(255,160,25) 0%, rgb(255,191,80) 100%);
	box-shadow: 0px 6px 0px 0px rgba(188, 121, 6, 0.76), 0 0 11px rgba(255,255,255,.24) inset
	left: 100px;
	top: 470px;
	width: 262px;
	height: 53px;
	line-height: 53px;
	font-size: 18px;
	font-family: "OpenSansBold", sans-serif;
	color: rgba(255, 255, 255, 0.98);
	text-shadow: 0px 0px 14.82px rgba(0, 0, 0, 0.2);
	text-align: center
	border-radius: 26.5px
	cursor pointer
	transition: all .3s ease
	&:focus
		background-image: linear-gradient( 0deg, rgb(255,160,25) 0%, rgb(255,191,80) 100%);
		color: rgba(255, 255, 255, 0.98);
		text-decoration: none
	&:hover
		background-image: linear-gradient( 180deg, rgb(255,160,25) 0%, rgb(255,191,80) 100%);
		color: rgba(255, 255, 255, 0.98);
		text-decoration: none
		transform: translateY(6px)
		box-shadow: none
h1
h2
h3
h4
h5
h6
	font-weight: normal
@import 'parts/main'
@import 'parts/for'
@import 'parts/plan'
@import 'parts/energies'
@import 'parts/results'
@import 'parts/interest'